<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sdau.superlibrary"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo3"
        android:label="@string/app_name"
        android:logo="@drawable/logo3"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sdau.activity.MainActivity"
            android:label="@string/app_name"
            android:logo="@drawable/logo3" >
        </activity>
        <activity android:name="com.sdau.activity.NewsInfoActivity" >
        </activity>
        <activity
            android:name="com.sdau.activity.UserLoginActivity"
            android:label="@string/title_activity_user_login" >
        </activity>
        <activity
            android:name="com.sdau.activity.SearchBookActivity"
            android:label="@string/title_activity_search_book" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sdau.activity.BookListActivity"
            android:label="@string/title_activity_book_list"
            android:theme="@style/titlebarstyle" >
        </activity>
        <activity
            android:name="com.sdau.activity.BookInfoActivity"
            android:label="@string/title_activity_book_info" >
        </activity>
    </application>

</manifest>